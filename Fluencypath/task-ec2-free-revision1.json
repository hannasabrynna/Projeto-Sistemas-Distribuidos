{
    "containerDefinitions": [
        {
            "name": "fluencypath-ecr",
            "image": "960005778154.dkr.ecr.us-east-1.amazonaws.com/fluencypath-ecr:latest",
            "cpu": 256,
            "memory": 256,
            "portMappings": [
                {
                    "name": "lavarel-port",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [
                { "name": "DB_HOST", "value": "db" },
                { "name": "DB_PORT", "value": "3306" },
                { "name": "DB_DATABASE", "value": "fluency_path" },
                { "name": "DB_USERNAME", "value": "fluency_path" },
                { "name": "DB_PASSWORD", "value": "password_mysql" }
            ],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "ulimits": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/task-ec2-free",
                    "awslogs-create-group": "true",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                },
                "secretOptions": []
            },
            "systemControls": [],
            "links": [
                "db"
            ]
        },
        {
            "name": "db",
            "image": "mysql:5.7",
            "cpu": 256,
            "memory": 600,
            "portMappings": [
                {
                    "containerPort": 3306,
                    "hostPort": 3307,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [
                { "name": "MYSQL_ROOT_PASSWORD", "value": "password_mysql" },
                { "name": "MYSQL_DATABASE", "value": "fluency_path" },
                { "name": "MYSQL_USER", "value": "fluency_path" },
                { "name": "MYSQL_PASSWORD", "value": "password_mysql" }
            ],
            "mountPoints": [
                {
                    "sourceVolume": "db-data",
                    "containerPath": "/var/lib/mysql",
                    "readOnly": false
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/task-ec2-free",
                    "awslogs-create-group": "true",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                },
                "secretOptions": []
            },
            "systemControls": []
        }
    ],
    "family": "task-ec2-free",
    "executionRoleArn": "arn:aws:iam::960005778154:role/ecsTaskExecutionRole",
    "networkMode": "bridge",
    "volumes": [
        {
            "name": "db-data",
            "host": {
                "sourcePath": "/ecs/db-data"
            }
        }
    ],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "EC2"
    ],
    "cpu": "512",
    "memory": "856",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    },
    "tags": [
        {
            "key": "Name",
            "value": "task-ec2-free"
        }
    ]
}